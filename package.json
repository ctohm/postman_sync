{
  "name": "@ctohm/postman_sync",
  "version": "1.1.0",
  "description": "This package deals with postman and openapi specs and tests",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:ctohm/postman_sync.git"
  },
  "author": "ffflabs <amenadiel@gmail.com>",
  "license": "MIT",
   "scripts": {
    "lint": "eslint server",
    "postman": "yarn postman:download && yarn swagger && yarn postman:export",
    "postman:download": "DEBUG=* node index.js --download",
    "postman:export": "DEBUG=pm_sync:* node index.js --export",
    "postman:inspect": "DEBUG=pm_sync:* node index.js --inspect",
    "jsonschema": "DEBUG=pm_sync:* node index.js --jsonschema",
    "postman:upload": "DEBUG=* node index.js --upload",
    "postman:inspect_upload": "yarn postman:inspect && yarn postman:upload",
    "swagger": "DEBUG=pm_sync:* node index.js --swagger output/specs/swagger/swagger.json",
    "generate_types": "DEBUG=dtsgen dtsgen -o output/specs/json-schema/types.d.ts output/specs/openapi/openapi-schema.json",
    "swagger:all": "yarn swagger && yarn jsonschema && yarn generate_types"
  },
  "bin": {
    "pm_sync": "./lib/cli.js"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "@bb-cli/json-schema-to-typedef": "^0.4.1",
    "@openapi-contrib/openapi-schema-to-json-schema": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "~7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.2",
    "minimist": "^1.2.5",
    "prettier": "^2.0.2"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^9.0.1",
    "@apidevtools/swagger-parser": "^10.0.2",
    "bluebird": "^3.7.2",
    "chalk": "^4.1.0",
    "debug": "^4.1.1",
    "deepdash": "^5.3.1",
    "dotenv": "^8.2.0",
    "dtsgenerator": "^3.0.3",
    "js-yaml": "^3.13.1",
    "json-schema-to-typescript": "^9.1.0",
    "lodash": "^4.17.15",
    "mkdirp": "^1.0.3",
    "node-fetch": "^2.6.0",
    "openapi-types": "^7.0.1",
    "postman-collection": "^3.6.2",
    "swagger2-postman2-parser": "^1.0.5",
    "swagger2openapi": "^7.0.3",
    "tv4": "^1.3.0",
    "wget-improved": "^3.2.0",
    "yaml": "^1.8.2"
  }
}
